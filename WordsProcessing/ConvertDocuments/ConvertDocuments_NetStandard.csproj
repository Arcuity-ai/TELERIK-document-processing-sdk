  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="Properties\AssemblyInfo.cs" />
  </ItemGroup>
  <ItemGroup>
    <None Remove="ConvertDocuments.csproj.vspscc" />
    <None Remove="ConvertDocuments_NetStandard.csproj.vspscc" />
